<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.fangsfmac.customview.progressLoading_01.ProgressLoadingActivity">


    <com.example.fangsfmac.customview.progressLoading_01.ProgressLoadView
        android:layout_width="300dp"
        android:layout_height="300dp"
        android:id="@+id/progressView"
        android:layout_gravity="center"
        app:centerTextSize="25sp"
        android:layout_marginTop="50dp"
        app:innerColor="@color/colorPrimaryDark"
        app:outColor="#f00" />

    <Button
        android:layout_width="match_parent"
        android:onClick="click1"
        android:text="测试"
        android:layout_height="wrap_content" />



</LinearLayout>
