<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ProgressLoadView">
        <attr name="innerColor" format="color" />
        <attr name="outColor" format="color" />
        <attr name="centerTextColor" format="color" />
        <attr name="centerTextSize" format="dimension" />
        <attr name="borderSize" format="dimension" />
    </declare-styleable>

    <declare-styleable name="ColorTrackTextView">
        <attr name="originColor" format="color" />
        <attr name="changeColor" format="color" />
    </declare-styleable>

    <declare-styleable name="ProgressView">
        <attr name="innerColor1" format="color" />
        <attr name="outColor1" format="color" />
        <attr name="borderWidth1" format="dimension" />
        <attr name="centerTextColor1" format="color" />
        <attr name="centerTextSize1" format="dimension" />
    </declare-styleable>


    <declare-styleable name="RatingBarView">
        <attr name="starNormalBg" format="reference" />
        <attr name="starFocusBg" format="reference" />
        <attr name="gradeNumber" format="integer" />
    </declare-styleable>


    <declare-styleable name="PowerProgressView">
        <attr name="powerInnerColor" format="color" />
        <attr name="powerOutColor" format="color" />
        <attr name="powerBorderWidth" format="dimension" />
        <attr name="powerCenterTextColor" format="color" />
        <attr name="powerCenterTextSize" format="dimension" />
        <!--外部轮廓线的颜色-->
        <attr name="powerOutlineColor" format="color" />
        <attr name="powerOutlineBorderWidth" format="dimension" />
        <attr name="powerDesTextColor" format="color" />
        <attr name="powerDesTextSize" format="dimension" />
        <attr name="powerDesText" format="string" />
        <attr name="powerTextPadding" format="dimension" />
    </declare-styleable>

    <declare-styleable name="LetterIndexView">
        <attr name="letterDefaultColor" format="color" />
        <attr name="letterSelectedColor" format="color" />
        <attr name="letterTextSize" format="dimension" />
    </declare-styleable>


    <declare-styleable name="SlideMenuLayout" >
        <attr name="menuRightMargin" format="dimension" />
    </declare-styleable>

    <declare-styleable name="QQSlideMenuLayout" >
        <attr name="QQmenuRightMargin" format="dimension" />
    </declare-styleable>
</resources>